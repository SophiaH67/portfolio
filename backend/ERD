# # Default Laravel stuff idc about
# password_resets
# ---
# email INDEX string
# token string
# created_at NULL datetime

# failed_jobs
# ---
# id UNIQUE int
# uuid UNIQUE string
# connection text
# queue text
# payload longText
# exception longText
# failed_at datetime

# personal_access_tokens
# ---
# id INDEX UNSIGNED_BIGINT
# tokenable_type VARCHAR
# tokenable_id UNSIGNED_BIGINT
# name string
# token UNIQUE string
# abilities NULL text
# last_used_at NULL datetime
# created_at datetime
# updated_at datetime

# Default laravel User model
users
---
id INDEX int
name string
admin boolean
email UNIQUE string
email_verified_at string
password string
token string
created_at datetime
updated_at datetime

projects
---
id INDEX int
description string
image string
created_at datetime
updated_at datetime
created_by int FK -< users.id
updated_by int FK -< users.id
